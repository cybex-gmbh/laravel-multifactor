{
    "name": "cybex/laravel-multi-factor",
    "description": "This package provides a flexible multi-factor authentication solution for Laravel, supports multiple multi-factor authentication methods and easy configuration of multi-factor authentication modes like optional, required or force.",
    "keywords": [
        "cybex",
        "laravel-multi-factor"
    ],
    "homepage": "https://github.com/cybex-gmbh/laravel-multi-factor",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Fabian Holy",
            "email": "fabian.holy@cybex-online.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^11.0",
        "laravel/fortify": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.12.0 ",
        "phpunit/phpunit": "^10"
    },
    "autoload": {
        "psr-4": {
            "Cybex\\LaravelMultiFactor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cybex\\LaravelMultiFactor\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cybex\\LaravelMultiFactor\\MultiFactorServiceProvider"
            ],
            "aliases": {
                "LaravelMultiFactor": "MultiFactorFacade"
            }
        }
    }
}
